    "test:password-reset:generate": "vitest run src/routes/auth/password-reset/generate-token.test.ts",
    "test:password-reset:invalid-email": "vitest run src/routes/auth/password-reset/invalid-email.test.ts",
    "test:password-reset:rate-limit": "vitest run src/routes/auth/password-reset/rate-limit.test.ts",
    "test:password-reset:security": "vitest run src/routes/auth/password-reset/security.test.ts",
    "test:auth": "vitest run src/routes/auth"
