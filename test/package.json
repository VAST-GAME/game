    "test:coverage": "vitest run --coverage",
    "test:register:all": "vitest run src/routes/auth/register",
    "test:register:valid": "vitest run src/routes/auth/register/valid-registration.test.ts",
    "test:register:invalid-email": "vitest run src/routes/auth/register/invalid-email.test.ts",
    "test:register:weak-password": "vitest run src/routes/auth/register/weak-password.test.ts",
    "test:register:duplicate": "vitest run src/routes/auth/register/duplicate-email.test.ts",
    "test:register:validation": "vitest run src/routes/auth/register/input-validation.test.ts",
    "test:login:all": "vitest run src/routes/auth/login",
    "test:login:valid": "vitest run src/routes/auth/login/valid-login.test.ts",
    "test:login:invalid": "vitest run src/routes/auth/login/invalid-credentials.test.ts",
    "test:login:nonexistent": "vitest run src/routes/auth/login/nonexistent-user.test.ts",
    "test:login:validation": "vitest run src/routes/auth/login/input-validation.test.ts",
    "test:protected:all": "vitest run src/routes/auth/protected",
